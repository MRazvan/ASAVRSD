<Window x:Class="SoftwareDebuggerExtension.About.AboutDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="About Software Debugger" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="auto"/>            
        </Grid.RowDefinitions>
        <WebBrowser Grid.Row="0" x:Name="WebBrowser"></WebBrowser>
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Margin="5" Width="70" Click="Button_Click">Ok</Button>
        </StackPanel>

    </Grid>
</Window>
